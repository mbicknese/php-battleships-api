{
    "name": "yoshz/symfony-docker-bootstrap",
    "type": "project",
    "autoload": {
        "psr-4": { "App\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "App\\Tests\\": "tests/" }
    },
    "require": {
        "symfony/symfony": "3.2.*",
        "doctrine/doctrine-bundle": "1.6.*",
        "doctrine/orm": "2.5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "phploc/phploc": "^3.0",
        "squizlabs/php_codesniffer": "^2.7"
    }
}
